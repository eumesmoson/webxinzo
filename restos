var datos;
var cont=0;
function remoteData(b,c,opc){
    //alert(b+" "+c+" "+opc);


    $.ajax({
        type:  'POST',
        url:   'datoseconomia.php',
        data:  {"mes":b,"ano":c,"opcion":opc,},
         }).done(function( msg ) {
         //alert(msg);
         //datos=msg.split(",");
         //alert(datos[0]);
         $(document).ajaxSuccess(function(){
         
         localStorage.setItem("datos",msg);
         $("#tituloquesito").html($("#lenda").html()); 

    });
         }).fail(function (jqXHR, textStatus, errorThrown){
          alert("Produciuse un erro: "+ textStatus +" "+ errorThrown);
        });
    
    
    datos=localStorage.getItem("datos").split(",");
    /*if(localStorage.getItem("datos")!=null){
        localStorage.clear("datos");
    }*/
    //alert(datos[0]);    
    //$("#tituloquesito").html("hola "+cont);
    salesData=[
    {label:"Agricultura/Pesca", color:"#3366CC",count:datos[0]},
    {label:"Industria", color:"#DC3912",count:datos[1]},
    {label:"Construcción", color:"#FF9900",count:datos[2]},
    {label:"Servizos", color:"#109618",count:datos[3]},
    {label:"Sen emprego anterior", color:"#EFE836",count:datos[4]}];
    
    
    //alert(datos[3]);
    function Data(){

    return salesData.map(function(d){ 
        return {label:d.label, value:d.count, color:d.color};});
    }

   
    Donut3D.transition("quotesDonut",Data(), 130, 100, 30, 0);
    //Donut3D.draw("quotesDonut", randomData(), 150, 130, 130, 100, 30, 0);
      
    
}


